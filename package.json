{
  "name": "gatsby-starter-basic",
  "description": "Basic, clean Gatsby starter for Inlight static websites",
  "version": "1.0.0",
  "author": "Inlight Media",
  "dependencies": {
    "classnames": "^2.2.5",
    "react-helmet": "^3.2.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "gatsby": "^0.12.20",
    "pre-commit": "^1.1.3",
    "sass-lint": "^1.10.2"
  },
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint": "eslint  --ignore-pattern 'public' --ext .js,.jsx .",
    "lint:sass": "./node_modules/.bin/sass-lint -v",
    "lint:staged": "LIST=`git diff --cached --name-only | grep '^.*\\.jsx*$'`; if [ \"$LIST\" ]; then eslint $LIST; fi",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "precommit": [
    "lint:staged"
  ]
}
